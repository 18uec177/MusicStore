@model IEnumerable<MusicStore.Models.GenreModel>
@{ViewBag.Title = "Store";}
<h3>Browse Genres</h3>
<p> Select from @Model.Count() genres:</p>

<ul>

    @foreach (var genre in Model)
    {

<a href="/Store/Browse/@genre.GenreId" class="card-body card-title  text-success text-decoration-none">@genre.Name</a> <br />           }
</ul>